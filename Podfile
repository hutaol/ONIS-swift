# Uncomment the next line to define a global platform for your project 
platform :ios, '9.0'

target 'OnisMobile-Swift' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OnisMobile-Swift

  pod 'Moya'
  # JSON和Model转换
  pod 'SwiftyJSON'
  pod 'HandyJSON'
  # 布局
  pod 'SnapKit'
  # 日期
  pod 'SwiftDate'
  # 图片异步加载
  pod 'Kingfisher'
  pod 'ESTabBarController-swift'
  pod 'IQKeyboardManagerSwift'
  
  # 数据库存储
  pod 'RealmSwift'
  # 响应式编程RxSwift
  pod 'RxSwift', '~> 5.0'
  pod 'RxCocoa', '~> 5.0'
  
  target 'OnisMobile-SwiftTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'OnisMobile-SwiftUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
